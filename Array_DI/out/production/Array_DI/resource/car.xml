<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN"
        "http://www.springframework.org/dtd/spring-beans-2.0.dtd">

<beans>

    <bean id="car" class="beans.Car">
      <property name="engineName">

          <list>
              <ref bean="e1"></ref>
              <ref bean="e2"></ref>
              <ref bean="e3"></ref>
          </list>
          
      </property>

        <property name="carName">
            <list>
                <value>c1</value>
                <value>c2</value>
                <value>c3</value>
            </list>
        </property>

    </bean>
    <bean id="e3"/>
    <bean id="e2"/>
    <bean id="e1"/>

</beans>